<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
<div  data-uri="{{ default _ref _self }}" data-editable="settings">
      <div class="title-ingridients-options">
              <button class="title-ingridients-options-button"> <i class="fa fa-heart"></i> Save</button>
              <button class="title-ingridients-options-button"> <i class="fa fa-user"></i> Made it</button>
              <button class="title-ingridients-options-button"> <i class="fa fa-star"></i> Rate it</button>
              <button class="title-ingridients-options-button"> <i class="fa fa-print"></i> Print</button>
              <button class="title-ingridients-options-button"> <i class="fa fa-pinterest"></i> Pin</button>
              <button class="title-ingridients-options-button"> <i class="fa fa-facebook"></i> Share</button>
      </div>
      <div class="title-ingridients">
                <div class="title-ingridients-title">
                      <h2 >Ingridients</h2>
                </div>
                <div class="title-ingridients-feat">
                        {{#if time}} <h3 class="feature title-ingridients-vdivider">Cook time: {{ time }} <i class="fa fa-clock-o"></i> </h3>{{/if}}
                        {{#if servings}} <h3 class="feature title-ingridients-vdivider">Servings: {{ servings }} <i class="fa fa-pie-chart"></i> </h3>{{/if}}
                        {{#if calories}} <h3 class="feature">Calories: {{ calories }} <i class="fa fa-bar-chart"></i> </h3>{{/if}}
                </div>
      </div>
      <div class="title-ingridients-divider">
              <div class="title-ingridients-container-list">
                <ul class="title-ingridients-list">
                        {{#each items as |item|}}
                        {{#if item.ingridients}}<li><span>{{{ item.ingridients }}}</span></li>{{/if}}
                        {{/each}}
                </ul>
                </div>
                <div class="title-ingridients-check">
                        <div class="title-ingdrients-sale">
                                <H2>On Sale</H2>
                                <h6>What's on sale near you.</h6>
                        </div>
                        <div class="title-ingdrients-settings">
                        <input type="checkbox" class="custom-control-input" id="customSwitches">
                        <label class="custom-control-label" for="customSwitches"> <i class="fa fa-edit"></i> </label>
                        </div>


                </div>
      </div>
</div>
